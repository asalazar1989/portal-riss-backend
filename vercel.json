{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "CLIENT_ID": "@client_id",
    "CLIENT_SECRET": "@client_secret",
    "TENANT_ID": "@tenant_id",
    "FILE_ID": "@file_id",
    "SHEET_NAME": "@sheet_name"
  }
}
